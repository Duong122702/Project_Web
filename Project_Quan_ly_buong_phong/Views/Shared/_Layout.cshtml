<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Project_Quan_ly_buong_phong</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/Project_Quan_ly_buong_phong.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-primary bg-white  box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Hotel InterContinental</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Phongs" asp-action="Index">Phòng</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Khachhangs">Khách hàng</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Bookings">Đặt Phòng</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Khachhangs">Khách hàng</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Chitietdvs">Chi Tiết Dịch Vụ</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Dskdps">Danh Sách Khách Đặt Phòng</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Hoadons">Hóa Đơn</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Dichvus">Dịch Vụ</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Thuocs">Thuộc</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="LogOut">Log Out</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer bg-primary text-muted">
        <div class="container text-center">
            Made with by Tung Duong, Thanh Long, Cao Duong
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
